{
  "family": "your-task-family",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::011528270926:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "your-container-nam",
      "image": "your-image-uri",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
      
    }
  ]
}
